   <string notr="true">/* ===================================================================
    Ultimate Purple Theme for PyQt5 by Gemini
    -------------------------------------------------------------------
    - This is a comprehensive stylesheet with explicit parameters.
    - Properties like font, color, background, border, padding,
      size, and alignment are detailed for each widget.
   =================================================================== */

/* === [1] Main Window &amp; Global Defaults === */
QWidget {
    /* Background and Font */
    background-color: #F5F3FF; /* Sets the background for the entire window */
    font-family: 'Segoe UI', 'Helvetica Neue', 'Arial', sans-serif; /* Primary font for the application */
    color: #3D315B; /* Default text color */
}

/* === [2] Title Labels (Specific Styling by objectName) === */
QLabel#label_title_train,
QLabel#label_title_predict {
    /* Font Styling */
    font-family: 'Segoe UI Semibold', 'Arial';
    font-size: 26px;
    font-weight: 600; /* Boldness: 400=normal, 600=semibold, 700=bold */
    font-style: normal;

    /* Color */
    color: #4C3B71; /* Dark, rich purple for high emphasis */
    background-color: transparent; /* Ensure it has no background color */

    /* Box Model */
    padding-top: 5px;
    padding-bottom: 5px;
    border: none; /* No border */
    border-bottom: 1px solid #D9D2E9; /* Subtle separator line */
    margin: 5px;

    /* Alignment (controlled via QSS) */
    qproperty-alignment: 'AlignHCenter'; /* Qt-specific property for alignment */
}


/* === [3] General Labels (e.g., 'Dataset Path:', 'Total Rooms:') === */
QLabel {
    /* Font Styling */
    font-size: 14px;
    font-weight: 500; /* Slightly bolder than normal */

    /* Color */
    color: #59516d; /* Softer purple-grey for readability */
    background-color: transparent;

    /* Box Model &amp; Sizing */
    padding-left: 5px;
    padding-right: 5px;
    min-height: 25px; /* Ensure labels don't get squished */
}

/* === [4] Input Fields (QLineEdit) === */
QLineEdit {
    /* Font Styling */
    font-size: 14px;
    font-weight: 400; /* Normal weight for input text */
    color: #333333; /* Dark text for high contrast */

    /* Color and Background */
    background-color: #FFFFFF; /* Bright white background */

    /* Box Model &amp; Border */
    border-style: solid;
    border-width: 1px;
    border-color: #D9D2E9; /* Light purple border */
    border-radius: 8px; /* Defines how rounded the corners are */
    padding: 8px 12px 8px 12px; /* Top, Right, Bottom, Left padding */

    /* Sizing */
    min-height: 25px; /* Minimum height for better clickability */
}

QLineEdit:focus {
    /* Style when the user has clicked into the input box */
    border-width: 2px;
    border-color: #9D81D9; /* A more prominent purple to indicate focus */
}


/* === [5] Primary Buttons ('Train Model', 'Browse', 'Predict') === */
QPushButton {
    /* Font Styling */
    font-size: 10pt;
    font-weight: 700; /* Bold text */
    color: #FFFFFF; /* High-contrast white text */

    /* Color and Background */
    background-color: #7E57C2; /* Main interactive purple */

    /* Box Model &amp; Border */
    border-style: none; /* No border for a modern, flat look */
    border-radius: 8px;
    padding: 10px 16px;

    /* Sizing */
    min-width: 80px;
    min-height: 25px;
}

QPushButton:hover {
    /* Style when the mouse cursor is over the button */
    background-color: #673AB7; /* Darker shade of purple */
}

QPushButton:pressed {
    /* Style when the button is being clicked */
    background-color: #512DA8; /* Even darker shade for click feedback */
}


/* === [6] Secondary Button ('OK') === */
QPushButton#btn_ok {
    /* Font Styling */
    font-size: 13px;
    font-weight: 600; /* Semibold, less prominent than primary buttons */
    color: #673AB7; /* Purple text to match the theme */

    /* Color and Background */
    background-color: #E8E2F9; /* Light lavender, less emphasis */

    /* Box Model &amp; Border */
    border: 1px solid #C5B9E8; /* Subtle border */
    border-radius: 6px; /* Slightly less rounded corners */
    padding: 5px 15px; /* Less vertical padding */

    /* Sizing */
    min-width: 70px;
    min-height: 20px;
}

QPushButton#btn_ok:hover {
    background-color: #D9D2E9;
    border-color: #B4A5DD;
}

QPushButton#btn_ok:pressed {
    background-color: #C5B9E8;
}

/* === [7] Status &amp; Result Labels (Specific Styling) === */
QLabel#label_status_train,
QLabel#label_price {
    font-size: 18px;
    font-weight: 600;
    color: #4C3B71;
    qproperty-alignment: 'AlignHCenter';
    padding: 5px;
}

