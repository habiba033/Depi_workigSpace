   <string notr="true">QMainWindow {
    background-color: #000000; /* الخلفية الرئيسية سوداء */
}
QPushButton:pressed, QPushButton:checked {
    background-color: #44bd32; /* green when clicked */
    color: white;}
/* Sidebar */
QListWidget {
    background-color: #1e1e1e;
    color: #ffffff;
    border-radius: 8px;
    font-size: 12pt;
}
QListWidget::item:selected {
    background: #00a8ff;
    color: white;
}
QListWidget::item:hover {
    background: #333333;
}

/* Tabs */
QTabBar::tab {
    font-size: 12pt;
    padding: 8px 20px;
    margin: 4px;
    border-radius: 12px;
    background: #1e1e1e;
    color: #bbbbbb;
}
QTabBar::tab:selected {
    background: #44bd32;
    color: white;
    font-weight: bold;
}
QTabBar::tab:hover {
    background: #4cd137;
    color: white;
}

/* Forms (LineEdits, Labels) */
QLabel {
    color: #ffffff;
    font-size: 11pt;
}
QLineEdit {
    font-size: 11pt;
    padding: 6px;
    border: 1px solid #444444;
    border-radius: 6px;
    background: #1e1e1e;
    color: #ffffff;
}
QLineEdit:focus {
    border: 2px solid #44bd32;
    background: #2c2c2c;
    color: #ffffff;
}

/* Buttons */
QPushButton {
    font-size: 11pt;
    font-weight: bold;
    padding: 8px 16px;
    border-radius: 8px;
    color: white;
    background-color: #1e1e1e;
    border: 1px solid #444;
}
QPushButton#add {
    background-color: #44bd32;
}
QPushButton#add:hover {
    background-color: #4cd137;
}
QPushButton#update {
    background-color: #0097e6;
}
QPushButton#update:hover {
    background-color: #00a8ff;
}
QPushButton#delete {
    background-color: #e84118;
}
QPushButton#delete:hover {
    background-color: #c23616;
}
/* ---------------- Buttons ---------------- */
QPushButton:pressed, QPushButton:checked {
    background-color: #44bd32; /* green when clicked */
    color: white;
}

/* ---------------- LineEdits ---------------- */
QLineEdit:focus {
    border: 2px solid #44bd32; /* green border when focused */
    background: #2c2c2c;
    color: #ffffff;
}

/* ---------------- QListWidget Items ---------------- */
QListWidget::item:selected {
    background-color: #44bd32; /* green when selected */
    color: white;
}

/* ---------------- QTabBar Tabs ---------------- */
QTabBar::tab:selected {
    background-color: #44bd32; /* green for selected tab */
    color: white;
    font-weight: bold;
}
/* ---------------- Date/Time Edits ---------------- */
QDateEdit, QDateTimeEdit, QTimeEdit {
    font-size: 11pt;
    padding: 6px;
    border: 1px solid #444444;
    border-radius: 6px;
    background: #1e1e1e;
    color: #ffffff;
    min-width: 120px;
}

/* Focus effect */
QDateEdit:focus, QDateTimeEdit:focus, QTimeEdit:focus {
    border: 2px solid #44bd32; /* green border */
    background: #2c2c2c;
    color: #ffffff;
}

/* Drop-down calendar (for QDateEdit / QDateTimeEdit) */
QCalendarWidget QWidget {
    background-color: #1e1e1e;
    color: #ffffff;
}

QCalendarWidget QToolButton {
    background-color: #333333;
    color: #ffffff;
    border: none;
    font-weight: bold;
    padding: 4px;
}

QCalendarWidget QToolButton:hover {
    background-color: #44bd32;
    color: white;
}

QCalendarWidget QAbstractItemView {
    background-color: #1e1e1e;
    color: #ffffff;
    selection-background-color: #44bd32;
    selection-color: white;
}

