    <string notr="true">/* ComboBox */
QComboBox {
    font-size: 11pt;
    padding: 6px 8px;
    border: 1px solid #444444;
    border-radius: 6px;
    background-color: #1e1e1e;
    color: #ffffff;
}

/* لما يبقى الفوكس على الكومبو */
QComboBox:focus {
    border: 2px solid #44bd32;
    background-color: #2c2c2c;
    color: #ffffff;
}

/* سهم النزول */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 28px;
    border-left: 1px solid #444444;
    background: transparent;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
}

/* شكل السهم الافتراضي */
QComboBox::down-arrow {
    image: none;   /* استخدم السهم الافتراضي للنظام */
    width: 10px;
    height: 10px;
}

/* اللستة اللي بتنزل */
QComboBox QAbstractItemView {
    background-color: #1e1e1e;
    border: 1px solid #444444;
    border-radius: 6px;
    selection-background-color: #44bd32;
    selection-color: white;
    color: #ffffff;
}
